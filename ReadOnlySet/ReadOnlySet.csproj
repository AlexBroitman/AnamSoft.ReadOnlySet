<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>ReadOnlySet is a generic read-only set. May be use as a read-only wrap on generic HashSet</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <RootNamespace>AnamSoft.ReadOnlySet</RootNamespace>
    <Authors>Alex Broitman</Authors>
    <Company>AnamSoft</Company>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>AnamSoft.ReadOnlySet</PackageId>
    <Copyright>AnamSoft</Copyright>
    <RepositoryUrl>https://github.com/AlexBroitman/AnamSoft.ReadOnlySet</RepositoryUrl>
    <Nullable>enable</Nullable>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <RepositoryType>Public</RepositoryType>
    <PackageTags>ReadOnlySet;HashSet</PackageTags>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>
